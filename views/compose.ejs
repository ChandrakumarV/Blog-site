<form action="/compose" method="post">
  <div class="mb-3">
    <label for="title" class="form-label">Title</label>
    <input name="title" type="text" class="form-control" id="title"/>
  </div>
  <div class="mb-3">
    <label for="body" class="form-label">Body</label>
